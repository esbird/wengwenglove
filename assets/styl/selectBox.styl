.selectBoxLine
    width: 365px
    max-height: 50vh
    // height 50vh
    overflow hidden
    border-radius: 8px
    background #fff
    // position fixed
    // left: 0
    // top: 0
    // bottom: 0
    // right: 0
    margin: 0 auto
    box-sizing: border-box
    display: flex;
    justify-content: center;
    // align-items: center;
    padding: 12px 0 20px
    .selectBox-wrap
        overflow auto
    ul
        // display flex
        // flex-direction: column
        font-size: 16px
        color: #E5CABF
        width: 337px
        li
            line-height: 45px
            border:1.5px solid #C9C9C9
            border-radius: 5px
            padding-left: 15px
            & ~li
                margin-top: 8px
            &.active
                background #E5CABF
                color #fff

